<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include ('partials/headNuevo') %>
    </head>
  <body>
    <%- include ('partials/navegacionNuevo') %>
      <div class="containerInfo">
        <main>
          <div class="container" style="margin-top: 8%;">
            <div class="row">
              <div class="col"></div>
              <div class="col-10">
                <h2 class="text">Parametros Generales</h2>
                <form class="needs-validation" novalidate>
                  <div class="form-row" style="margin-top: 5%;">
                    <div class="col-md-4 mb-3">
                      <label for="NombreCompaniaParamGenerales">Nombre de la Compañia</label>
                      <input type="text" class="form-control" id="NombreCompaniaParamGenerales" required>
                    </div>
                    <div class="col-md-4 mb-3">
                      <label for="CedulaJuridicaParamGenerales">Cedula Juridica</label>
                      <input type="number" class="form-control" id="CedulaJuridicaParamGenerales" required>
                    </div>
                    <div class="col-md-4 mb-3">
                        <label for="TelefonoParamGenerales">Telefono</label>
                        <input type="number" class="form-control" id="TelefonoParamGenerales" required>
                      </div>
                  </div>
                  <div class="form-row">
                    <div class="col-md-4 mb-3">
                      <label for="MensajeSaludoParamGenerales">Mensaje Saludo</label>
                      <div class="input-group">
                        <input type="text" class="form-control" id="MensajeSaludoParamGenerales" aria-describedby="inputGroupPrepend" required>
                      </div>
                    </div>
                    <div class="col-md-4 mb-3">
                      <label for="DireccionDelEstablecimiento">Direccion del establecimiento</label>
                      <input type="text" class="form-control" id="DireccionDelEstablecimiento" required>
                    </div>
                  </div>
                  <button class="btn btn-primary" type="submit">Agregar</button>
                  <button class="btn btn-warning" type="submit">Modificar</button>
                  <button class="btn btn-danger" type="submit">Eliminar</button>


                  <table class="table" style="margin-top: 10%;">
                    <thead class="thead-light">
                      <tr>
                        <th scope="col">Nombre de la Compañia</th>
                        <th scope="col">Cedula Juridica</th>
                        <th scope="col">Telefono</th>
                        <th scope="col">Mensaje Saludo</th>
                        <th scope="col">Direccion del establecimiento</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                    </tbody>
                  </table>

                </form>
               
              </div>
              <div class="col">
              </div>
            </div>
          </div>
        </main>
      </div>
    </div>
    <%- include ('partials/script') %>
  </body>
</html>

